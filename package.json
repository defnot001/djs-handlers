  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "discord.js": "^14.7.1",
    "glob": "^8.1.0"
  },
  "devDependencies": {
    "@types/glob": "^8.0.1",
    "@types/node": "^18.11.18",
    "ts-node": "^10.9.1",
    "tsup": "^6.5.0",
    "typescript": "^4.9.4"
  }
}
